{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "inject": true,
    "onmessage": true,
    "expect": true
  },
  "plugins": [],
  "rules": {
    "no-unused-expressions": 0,
    "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
    "require-yield": 0,
    "security/detect-object-injection": 0,
    "security/detect-non-literal-fs-filename": "off"
  }
}